
<ul>
  <% games.forEach(function (game) { %>
    <li>
      <button class="want-button" data-id="<%= game.id %>">
        <% if (game.myGame && game.myGame.get("status") === "wants-to-play") { %>
        unwant to play
        <% } else { %>
        want to play
        <% } %>
      </button>
      <ul>
        <li>title: <%= game.escape("title") %></li>
        <li>genere: <%= game.escape("genre") %></li>
        <li>company: <%= game.escape("company") %></li>
        <li>description: <%= game.escape("description") %></li>
        <li>created at: <%= game.escape("created_at") %></li>
        <li>updated at: <%= game.escape("updated_at") %></li>
      </ul>
    </li>
  <% }) %>
</ul>
